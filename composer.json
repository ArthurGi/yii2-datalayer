{
  "name": "dekar91/yii2-datalayer",
  "type": "yii2-extension",
  "description": "DataLayer integration for the Yii framework",
  "keywords": ["yii2", "datalayer", "helper"],
  "license": "MIT",
  "authors": [
    {
      "name": "Denis Karakoliuchka"
    }
  ],
  "support": {
    "issue": "https://github.com/dekar91/yii2-datalayer/issues"
  },
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.0"
  },
  "config": {
  "process-timeout": 1800,
  "fxp-asset":{
    "installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower-asset"
      }
    }
  },

  "autoload": {
    "psr-4": {
      "dekar91\\datalayer\\": ""
    }
  }
}